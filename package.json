{
  "name": "obsidian-kanban",
  "version": "2.0.51",
  "description": "",
  "main": "main.js",
  "scripts": {
    "typecheck": "tsc --noemit",
    "dev": "node esbuild.config.mjs",
    "build": "npm-run-all --serial build:*",
    "build:1": "node esbuild.config.mjs production",
    "build:2": "shx mkdir -p dist && shx cp main.js manifest.json styles.css dist/",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clean": "yarn prettier && yarn lint:fix",
    "rlnotes": "git log $(git describe --tags --abbrev=0)..HEAD --oneline > release-notes.md && git add release-notes.md",
    "bump": "node version-bump.mjs && git add package.json manifest.json versions.json && yarn rlnotes",
    "release": "git commit -m $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/commands": "6.10.0",
    "@codemirror/state": "6.5.2",
    "@codemirror/view": "6.39.0",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/box-intersect": "1.0.3",
    "@types/mark.js": "8.11.12",
    "@types/node": "24.10.2",
    "@types/raf-schd": "4.0.3",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "builtin-modules": "5.0.0",
    "esbuild-plugin-less": "1.3.33",
    "esbuild": "0.27.1",
    "eslint-plugin-react": "7.37.5",
    "eslint": "9.39.1",
    "magic-string": "0.30.21",
    "npm-run-all": "4.1.5",
    "obsidian-dataview": "0.5.68",
    "obsidian-plugin-cli": "0.9.0",
    "obsidian": "1.10.3",
    "prettier": "3.7.4",
    "replace": "1.2.2",
    "shx": "0.4.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tanstack/match-sorter-utils": "8.19.4",
    "@tanstack/react-table": "8.21.3",
    "animated-scroll-to": "2.3.2",
    "box-intersect": "1.0.2",
    "choices.js": "11.1.0",
    "classcat": "5.0.5",
    "colord": "2.9.3",
    "deepmerge": "4.3.1",
    "eventemitter3": "5.0.1",
    "fast-json-patch": "3.1.1",
    "file-selector": "2.1.2",
    "immutability-helper": "3.1.1",
    "immutable-json-patch": "6.0.2",
    "is-plain-object": "5.0.0",
    "mark.js": "8.11.1",
    "mdast-util-from-markdown": "2.0.2",
    "mdast-util-frontmatter": "2.0.1",
    "mdast-util-to-markdown": "2.1.2",
    "mdast-util-to-string": "4.0.0",
    "mdast": "3.0.0",
    "micromark-util-character": "2.1.1",
    "micromark-util-symbol": "2.0.1",
    "micromark-util-types": "2.0.2",
    "monkey-around": "3.0.0",
    "obsidian-daily-notes-interface": "0.9.4",
    "preact": "10.28.0",
    "react-colorful": "5.6.1",
    "react-cool-onclickoutside": "1.7.0",
    "react-dom": "npm:@preact/compat",
    "react-fast-compare": "3.2.2",
    "react": "npm:@preact/compat",
    "rrule": "2.8.1",
    "unist-util-visit": "5.0.0"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "module-alias",
      "options": {
        "alias": [
          "src"
        ]
      }
    }
  }
}
